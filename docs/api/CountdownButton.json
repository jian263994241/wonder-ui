{
  "description": "活动注册等短信验证码场景, 发送短信按钮\n@visibleName CountdownButton 短信倒计时",
  "displayName": "CountdownButton",
  "methods": [],
  "props": {
    "totail": {
      "type": {
        "name": "number"
      },
      "required": false,
      "description": "倒计时的秒数",
      "defaultValue": {
        "value": "60",
        "computed": false
      }
    },
    "defaultText": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "初始状态文本",
      "defaultValue": {
        "value": "'获取验证码'",
        "computed": false
      }
    },
    "defaultTextRe": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "初始状态文本",
      "defaultValue": {
        "value": "'重新发送'",
        "computed": false
      }
    },
    "text": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "倒计时状态的文本",
      "defaultValue": {
        "value": "'%ss'",
        "computed": false
      }
    },
    "onStart": {
      "type": {
        "name": "func"
      },
      "required": true,
      "description": "点击执行 done=>done()"
    },
    "runOnMount": {
      "type": {
        "name": "bool"
      },
      "required": false,
      "description": "DidMount时开始执行 onStart"
    },
    "render": {
      "type": {
        "name": "func"
      },
      "required": false,
      "description": "改变渲染的 node"
    }
  }
}