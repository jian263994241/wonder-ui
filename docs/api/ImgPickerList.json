{
  "description": "图片上传列表组件，属性同ImgPicker\n@visibleName ImgPickerList 图片上传列表",
  "displayName": "ImgPickerList",
  "methods": [],
  "props": {
    "fileDownLoad": {
      "type": {
        "name": "func"
      },
      "required": false,
      "description": "下载图片方法, 返回一个Promise方法",
      "defaultValue": {
        "value": "() => {}",
        "computed": false
      }
    },
    "onChange": {
      "type": {
        "name": "func"
      },
      "required": false,
      "description": "files 值发生变化触发的回调函数",
      "defaultValue": {
        "value": "() => {}",
        "computed": false
      }
    },
    "files": {
      "type": {
        "name": "array"
      },
      "required": false,
      "description": "图片列表"
    },
    "selectable": {
      "type": {
        "name": "bool"
      },
      "required": false,
      "description": "是否显示添加按钮",
      "defaultValue": {
        "value": "true",
        "computed": false
      }
    }
  }
}