{
  "name": "@wonder-ui/styled",
  "version": "1.0.3",
  "description": "Wonder-UI styles",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/styled.esm.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "tsdx watch --entry src/index.ts",
    "build": "tsdx build --entry src/index.ts",
    "lint": "tsdx lint src",
